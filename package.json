{
  "name": "midgard-react",
  "version": "0.0.1",
  "description": "Main frontend module for Walhall applications built using the React framework",
  "main": "src/index.js",
  "dependencies": {
    "enzyme-adapter-react-16": "^1.13.1",
    "freyja-react": "^0.1.14",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.2.0",
    "gulp-git": "^2.9.0",
    "jest-styled-components": "^6.3.1",
    "npm": "^6.9.0",
    "polished": "^3.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-notifications": "^1.4.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-styleguidist": "^9.0.8",
    "react-test-renderer": "^16.8.6",
    "reactjs-popup": "^1.4.0",
    "redux-saga": "^1.0.0",
    "serve": "^11.0.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "enzyme": "^3.8.0",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "gulp-header": "^2.0.7",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^23.6.0",
    "midgard-core": "^0.1.3",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "test": "jest  --updateSnapshot --coverage",
    "serve": "./node_modules/.bin/serve -s dist",
    "build": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "createFile": "npm run cleanUp && gulp createFile  --gulpfile gulpfile.js --cwd ./",
    "init": "gulp init --gulpfile gulpfile.js --cwd ./ && npm run createFile",
    "cleanUp": "gulp deleteIndex  --gulpfile gulpfile.js --cwd ./",
    "styleguidist": "npx styleguidist build && npx styleguidist server"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "ui/Menu": "<rootDir>/src/ui-components/Menu/menu",
      "ui/Button": "<rootDir>/src/ui-components/Button/Button",
      "ui/TextField": "<rootDir>/src/ui-components/TextField/TextField",
      "ui/InputField": "<rootDir>/src/ui-components/InputField/InputField",
      "ui/InlineEditor": "<rootDir>/src/ui-components/InlineEditor/InlineEditor"
    },
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src",
      "src",
      "<rootDir>/src/styles",
      "<rootDir>"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "keywords": [
    "Midgard",
    "React"
  ],
  "author": "Alex Percsi",
  "license": "ISC"
}
